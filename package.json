{
  "name": "online-music-store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "jest": "jest --watch",
    "server": "json-server db.json -m ./node_modules/json-server-auth",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start-app": "npm run server && npm run start",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular/animations": "~10.1.5",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/elements": "^10.2.0",
    "@angular/forms": "~10.1.5",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "document-register-element": "^1.7.2",
    "json-server-auth": "^2.0.2",
    "lit-element": "^2.4.0",
    "ngx-toastr": "^13.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.6",
    "@angular/cli": "~10.1.6",
    "@angular/compiler-cli": "~10.1.5",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-controls": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/angular": "^6.0.27",
    "@testing-library/angular": "^10.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.0",
    "es-module-loader": "^2.3.0",
    "jest": "^26.6.0",
    "jest-preset-angular": "^7.1.1",
    "json-server": "^0.16.2",
    "protractor": "~7.0.0",
    "react-is": "^17.0.1",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/src/test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
